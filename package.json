{
    "name": "vs-snippets",
    "displayName": "VS Snippets - Code Snippets Manager",
    "description": "âœ¨ Professional Code Snippets Manager for VS Code - Supercharge your productivity with organized code snippets",
    "version": "1.0.1",
    "publisher": "petercsipkay",
    "website": "https://vssnippets.com",
    "icon": "resources/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/petercsipkay/vssnippetscom"
    },
    "bugs": {
        "url": "https://github.com/petercsipkay/vssnippetscom/issues"
    },
    "homepage": "https://vssnippets.com",
    "author": {
        "name": "Peter Csipkay"
    },
    "license": "Commercial",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "Snippets",
        "Programming Languages",
        "Other"
    ],
    "keywords": [
        "snippets",
        "bookmark",
        "code",
        "bookmarks",
        "Code Blocks",
        "Collections",
        "Code Snippets",
        "Code Snippet Manager",
        "Code Snippet Library",
        "Code Snippet Templates",
        "Code Snippet Organization",
        "Code Snippet Reuse",
        "productivity",
        "code-library",
        "code-templates",
        "code-organization",
        "code-reuse",
        "folder-organization",
        "drag-and-drop",
        "backup",
        "sync",
        "cloud-sync"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "snippets-explorer",
                    "title": "Code Snippets Manager",
                    "icon": "resources/logo.svg"
                }
            ]
        },
        "views": {
            "snippets-explorer": [
                {
                    "id": "snippetsExplorer",
                    "name": "VS Snippets",
                    "icon": "resources/snippets.svg",
                    "dragAndDropController": true
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "snippets.addFolder",
                    "when": "view == snippetsExplorer",
                    "group": "navigation@1"
                },
                {
                    "command": "snippets.search",
                    "when": "view == snippetsExplorer",
                    "group": "navigation@2"
                },
                {
                    "command": "snippets.clearSearch",
                    "when": "view == snippetsExplorer && snippets:hasSearchQuery",
                    "group": "navigation@3"
                },
                {
                    "command": "snippets.manageSettings",
                    "when": "view == snippetsExplorer",
                    "group": "navigation@4"
                }
            ],
            "view/item/context": [
                {
                    "command": "snippets.addSubfolder",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "inline@1"
                },
                {
                    "command": "snippets.addSnippet",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "inline@2"
                },
                {
                    "command": "snippets.deleteItem",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "inline@6"
                },
                {
                    "command": "snippets.deleteItem",
                    "when": "view == snippetsExplorer && viewItem == snippet",
                    "group": "inline"
                },
                {
                    "command": "snippets.addSubfolder",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "1_creation@1"
                },
                {
                    "command": "snippets.addSnippet",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "1_creation@2"
                },
                {
                    "command": "snippets.renameFolder",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "1_modification"
                },
                {
                    "command": "snippets.renameSnippet",
                    "when": "view == snippetsExplorer && viewItem == snippet",
                    "group": "1_modification"
                },
                {
                    "command": "snippets.moveToRoot",
                    "when": "view == snippetsExplorer && viewItem == subfolder",
                    "group": "2_modification@1"
                },
                {
                    "command": "snippets.moveUp",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "2_modification@2"
                },
                {
                    "command": "snippets.moveDown",
                    "when": "view == snippetsExplorer && (viewItem == folder || viewItem == subfolder)",
                    "group": "2_modification@3"
                }
            ]
        },
        "commands": [
            {
                "command": "snippets.search",
                "title": "Search Snippets",
                "icon": "$(search)"
            },
            {
                "command": "snippets.clearSearch",
                "title": "Clear Search",
                "icon": "$(clear-all)"
            },
            {
                "command": "snippets.manageSettings",
                "title": "Manage Settings",
                "icon": "$(gear)"
            },
            {
                "command": "snippets.addFolder",
                "title": "Add Folder",
                "icon": "$(add)"
            },
            {
                "command": "snippets.addSubfolder",
                "title": "Add Subfolder",
                "icon": "$(new-folder)"
            },
            {
                "command": "snippets.addSnippet",
                "title": "Add Snippet",
                "icon": "$(new-file)"
            },
            {
                "command": "snippets.deleteItem",
                "title": "Delete",
                "icon": "$(trash)"
            },
            {
                "command": "snippets.exportSnippets",
                "title": "Export Snippets",
                "icon": "$(export)"
            },
            {
                "command": "snippets.importSnippets",
                "title": "Import Snippets",
                "icon": "$(import)"
            },
            {
                "command": "snippets.syncFromBackup",
                "title": "Sync from Backup Folder",
                "icon": "$(sync)"
            },
            {
                "command": "snippets.renameFolder",
                "title": "Rename Folder",
                "icon": "$(edit)"
            },
            {
                "command": "snippets.renameSnippet",
                "title": "Rename Snippet",
                "icon": "$(edit)"
            },
            {
                "command": "snippets.configureBackupFolder",
                "title": "Configure Backup Folder",
                "category": "VS Snippets"
            },
            {
                "command": "snippets.openHelpWebsite",
                "title": "Get Help at vssnippets.com",
                "category": "VS Snippets"
            },
            {
                "command": "snippets.moveToRoot",
                "title": "Move to Root Level",
                "icon": "$(root-folder)"
            },
            {
                "command": "snippets.moveUp",
                "title": "Move Up",
                "icon": "$(arrow-up)"
            },
            {
                "command": "snippets.moveDown",
                "title": "Move Down",
                "icon": "$(arrow-down)"
            },
            {
                "command": "snippets.enterLicense",
                "title": "Enter License Key",
                "category": "VS Snippets"
            }
        ],
        "configuration": {
            "title": "VS Snippets",
            "properties": {
                "snippets.backupFolder": {
                    "type": "string",
                    "default": "",
                    "description": "The folder path where your snippets will be backed up. For cross-computer sync, choose a cloud storage folder (Dropbox, Google Drive, etc.)"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/node": "^16.18.123",
        "@types/vscode": "^1.87.0",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "eslint": "^8.54.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "monaco-editor": "^0.52.2"
    }
}